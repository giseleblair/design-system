{
  "name": "@zesty-io/core",
  "version": "0.15.0",
  "description": "This is a collection of React core UI components for the Zesty.io platform.",
  "author": "Zesty.io <developers@zesty.io>",
  "license": "UNLICENSED",
  "main": "dist/",
  "browser": "dist/umd.bundle.js",
  "scripts": {
    "test": "npx jest ./src/**/*test.js --config config.jest.js",
    "precommit": "pretty-quick --staged",
    "build": "npm run build:es2015 && npm run build:bundle && npx gulp --gulpfile config.gulp.js && cp package.json dist/package.json",
    "build:es2015": "cross-env NODE_ENV=production npx babel ./src --out-dir ./dist --ignore *.test.js",
    "build:bundle": "cross-env NODE_ENV=production npx rollup -c config.rollup.js",
    "clean": "npx rimraf dist",
    "release": "npm run clean && npm run build && cd dist && npm publish",
    "start": "npm i && npm run build && cd dist/ && npm link"
  },
  "dependencies": {
    "codemirror": "^5.49.0",
    "js-beautify": "^1.10.2"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "react": "^16.10.2",
    "react-router-dom": "^4.3.1",
    "react-dom": "^16.10.2",
    "moment": "^2.22.2",
    "@aeaton/react-prosemirror": "^0.21.1",
    "@aeaton/react-prosemirror-config-default": "^0.11.0",
    "codemirror": "^5.48.0",
    "react-codemirror2": "^5.1.0",
    "showdown": "^1.9.0",
    "uuid": "^3.3.2",
    "flatpickr": "^4.5.2",
    "react-flatpickr": "^3.6.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.2",
    "babel-jest": "24.9.0",
    "classnames": "2.2.6",
    "cross-env": "6.0.3",
    "gulp": "4.0.2",
    "gulp-concat-css": "3.1.0",
    "husky": "3.0.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "less": "3.10.3",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-test-renderer": "16.10.2",
    "rollup": "1.23.1",
    "rollup-plugin-analyzer": "3.2.1",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy-glob": "0.3.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-postcss-modules": "2.0.1",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-size-snapshot": "0.10.0"
  }
}
